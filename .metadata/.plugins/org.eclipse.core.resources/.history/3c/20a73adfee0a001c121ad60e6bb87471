package com.anitesh.persistance;

import org.springframework.data.jpa.repository.JpaRepository;
import org.springframework.data.jpa.repository.Query;
import org.springframework.stereotype.Repository;

import com.anitesh.bean.InventoryItem;

@Repository
public interface InventoryItemDoa extends JpaRepository<InventoryItem, Integer>{
	
	@Query("Select id,avaiableQuantity,avaiableQuantity from InventoryItem where avaiableQuantity=:code ")
	public InventoryItem getInventoryItemByCode(String code);


}
